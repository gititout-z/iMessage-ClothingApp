IPHONEOS_DEPLOYMENT_TARGET = 15.0

// Signing
CODE_SIGN_STYLE = Automatic
DEVELOPMENT_TEAM = 

// CloudKit
CLOUDKIT_CONTAINER_IDENTIFIER = iCloud.com.yourcompany.clothingapp
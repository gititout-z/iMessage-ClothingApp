<!-- Release.xcconfig -->
// Release configuration

// Bundle Identifier
PRODUCT_BUNDLE_IDENTIFIER = com.yourcompany.clothingapp.messages

// Build Settings
DEBUG_INFORMATION_FORMAT = dwarf-with-dsym
ENABLE_NS_ASSERTIONS = NO
GCC_OPTIMIZATION_LEVEL = s
